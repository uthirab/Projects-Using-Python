#GCD
import math
print("Calculates the GCD of two numbers\n")
a=int(input("Enter the first number: \n"))
b=int(input("enter the second number: \n"))
gcd= str(math.gcd(a,b))
print("GCD is " +gcd)
      
#LCM
import math
print("Calculates the LCM of two numbers\n")
a=int(input("Enter the first number: \n"))
b=int(input("enter the second number: \n"))
gcd= math.gcd(a,b)
lcm= str(a*b/gcd)
print("LCM is " +lcm)
      
